{
  "newsnip": {
    "prefix": "newsnip",
    "body": [
      "\"${1:trigger}\": {",
      "\"prefix\": \"${1:trigger}\"",
      "\"body\": [",
      "${2:body}",
      "]",
      "\"description\": \"${3:Description}\""
    ],
    "description": "Create a new JSON snippet template"
  },
  "snipfile": {
    "prefix": "snipfile",
    "body": [
      "{",
      "  \"${1:snippet_name}\": {",
      "    \"prefix\": \"${2:trigger}\",",
      "    \"body\": [",
      "      ${3:\"line1\",}",
      "      ${4:\"line2\"}",
      "    ],",
      "    \"description\": \"${5:description}\"",
      "  }",
      "}"
    ],
    "description": "Create a new JSON snippets file template"
  }
}
